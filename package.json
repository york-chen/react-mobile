    "cross-env": "^7.0.3",
    "dotenv": "^16.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.20.0",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
