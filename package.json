{
  "name": "mobile_web",
  "version": "1.0.0",
  "description": "移动端项目",
  "main": "index.js",
  "scripts": {
    "start": "webpack server --env mode=development --config ./build/webpack.config.js",
    "build:dll:react_bucket": "webpack --env mode=production --config build/react-bucket.dll.js",
    "build:dll:react_plugin": "webpack --env mode=production --config build/react-plugin.dll.js",
    "build:dll:third_part": "webpack --env mode=production --config build/third-part.dll.js",
    "build:dll:all": "yarn build:dll:react_bucket && yarn build:dll:react_plugin && yarn build:dll:third_part",
    "build": "cross-env PROJECT_MODE=pro webpack --env mode=production --config ./build/webpack.config.js"
  },
  "keywords": [
    "antd、webapp"
  ],
  "author": "york",
  "license": "ISC",
